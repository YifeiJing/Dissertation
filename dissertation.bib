journal = {Proceedings of the IEEE},
keywords = {Electric vehicles (EVs),gridable electric vehicles (GEVs),power grid,renewable energy,smart grid,smart home,vehicle to grid (V2G),vehicle to home (V2H),vehicle to vehicle (V2V)},
mendeley-groups = {EV and curtailment,EV and curtailment/details},
number = {11},
