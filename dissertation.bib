file = {:Users/jingyifei/Documents/Essays/paper221119/05471115.pdf:pdf},
issn = {08858950},
journal = {IEEE Transactions on Power Systems},
keywords = {Distribution investment,distribution network planning,electricity distribution,network energy losses,plug-in electric vehicles},
mendeley-groups = {EV and curtailment/details},
number = {1},
pages = {206--213},
title = {{Assessment of the impact of plug-in electric vehicles on distribution networks}},
volume = {26},
year = {2011}
}
@article{paper:windstructure,
 ISSN = {0309524X, 2048402X},
 URL = {http://www.jstor.org/stable/43749036},
 abstract = {The properties of the Weibull distribution function are used to show that there are some simple relationships between the Weibull parameters (k, c) and the wind speed moments $(\bar v,\,\overline {{v^3}} )$. In particular, to within 1%, v̄ = 1/2π1/2c (for 1.6 ≤ k ≤ 3.0) and $\,\overline {{v^3}} = \,1/2{\pi ^{1/2}}(3{c^3}/k)$ (for 1.8 ≤ k 2.3). These results are subsequently used to examine the relationship between seasonal, yearly and many-yearly Weibull parameters for a given site. Some simple 'sum-rules' are derived which conserve the time-averaged moments v̄ and $\,\overline {{v^3}} $. These sum rules are used, in turn, to suggest a 'generalised Weibull distribution function' which can be used to model the fluctuations, which will occur from year to year, in the average moments v̄ and $\,\overline {{v^3}} $. In addition, some new results for Lord Howe Island are presented, which show that the annual wind speed, at selected sites, is ~8.1 ms⁻¹. These results are used to discuss the problem of determining the Weibull parameters (k, c) from experiment. It is shown that the 'maximum-likeness' and 'moments' methods are superior to the 'ℓnℓn linear least squares' method. Finally some comments are also made concerning the choice of distribution function to model wind speed statistics. Using Pearson's method of moments, it is shown that a Pearson distribution function type IB could be used to model the Lord Howe island wind data, in preference to the Weibull distribution function. However the former is rejected on the grounds that the use of three parameters to model the wind speed statistics does not lead to any significant improvement over the two parameter Weibull distribution function.},
 author = {G.J. Bowden and P.R. Barker and V.O. Shestopal and J.W. Twidell},
 journal = {Wind Engineering},
 number = {2},
 pages = {85--98},
 publisher = {Sage Publications, Ltd.},
 title = {The Weibull Distribution Function and Wind Power Statistics},
 volume = {7},
 year = {1983}
}
