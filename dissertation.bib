file = {:Users/jingyifei/Documents/Essays/paper221119/05484336.pdf:pdf},
isbn = {9781424465477},
journal = {2010 IEEE PES Transmission and Distribution Conference and Exposition: Smart Solutions for a Changing World},
keywords = {Customer behavior,Demand response,PHEV,Smart grid,Time-of-use rate},
